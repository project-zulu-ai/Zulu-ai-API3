modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FastAPI App Generator"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Zulu API"

[[workflows.workflow]]
name = "FastAPI App Generator"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install fastapi uvicorn jinja2 pydantic && python main.py"
waitForPort = 8000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Zulu API"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8000
externalPort = 80
